          "name": "system0.0"
        },
        {
          "name": "ethernet-1/1.0"
        },
        {
          "name": "ethernet-1/2.0"
        }
      ],
      "protocols": {
        "bgp": {
          "admin-state": "enable",
          "autonomous-system": 4200000100,
          "router-id": "10.0.0.13",
          "afi-safi": [
