              "router-role": {
                "admin-state": "enable"
              }
            }
          }
        }
      ]
    }
  ],
  "routing-policy": {
    "prefix-set": [
      {
        "name": "system-loopbacks",
        "prefix": [
          {
            "ip-prefix": "10.0.0.0/8",
            "mask-length-range": "32..32"
          }
        ]
      }
    ],
    "policy": [
      {
        "name": "system-loopbacks-policy",
        "statement": [
          {
            "name": "1",
            "match": {
              "prefix-set": "system-loopbacks"
            },
            "action": {
              "policy-result": "accept"
            }
          }
        ]
      }
    ]
  },
  "network-instance": [
    {
      "name": "default",
      "interface": [
