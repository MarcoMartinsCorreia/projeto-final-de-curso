              "afi-safi-name": "ipv4-unicast",
              "admin-state": "enable"
            },
            {
              "afi-safi-name": "evpn",
              "admin-state": "enable"
            }
          ],
          "dynamic-neighbors": {
            "interface": [
              {
                "interface-name": "ethernet-1/1.0",
                "peer-group": "underlay",
                "allowed-peer-as": [4200000111]
              },
              {
                "interface-name": "ethernet-1/2.0",
                "peer-group": "underlay",
                "allowed-peer-as": [4200000112]
              }
            ]
          },
          "group": [
            {
              "group-name": "underlay",
              "admin-state": "enable",
              "export-policy": "system-loopbacks-policy",
              "import-policy": "system-loopbacks-policy"
            },
            {
              "group-name": "ibgp-overlay-rr-clients",
              "admin-state": "enable",
              "peer-as": 65535,
              "local-as": {
                "as-number": 65535
              },
              "route-reflector": {
                "client": true,
                "cluster-id": "10.0.0.13"
              },
              "afi-safi": [
                {
                  "afi-safi-name": "ipv4-unicast",
                  "admin-state": "disable"
                },
                {
                  "afi-safi-name": "evpn",
                  "admin-state": "enable"
                }
              ]
            }
          ],
          "neighbor": [
            {
              "peer-address": "10.0.0.11",
              "peer-group": "ibgp-overlay-rr-clients",
              "transport": {
                "local-address": "10.0.0.13"
              }
            },
            {
              "peer-address": "10.0.0.12",
              "peer-group": "ibgp-overlay-rr-clients",
              "transport": {
                "local-address": "10.0.0.13"
