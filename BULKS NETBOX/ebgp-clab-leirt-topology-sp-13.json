{
  "interface": [
    {
      "name": "system0",
      "description": "system0 interface on clab-leirt-topology-sp-13 node",
      "admin-state": "enable",
      "subinterface": [
        {
          "index": 0,
          "admin-state": "enable",
          "ipv4": {
            "admin-state": "enable",
            "address": [
              {
                "ip-prefix": "10.0.0.13/32"
              }
            ]
          }
        }
      ]
    },
    {
      "name": "ethernet-1/1",
      "description": "ethernet-1/1 interface on clab-leirt-topology-sp-13 node",
      "admin-state": "enable",
      "subinterface": [
        {
          "index": 0,
          "admin-state": "enable",
          "ipv6": {
            "admin-state": "enable",
            "router-advertisement": {
              "router-role": {
                "admin-state": "enable"
              }
            }
          }
        }
      ]
    },
    {
      "name": "ethernet-1/2",
      "description": "ethernet-1/2 interface on clab-leirt-topology-sp-13 node",
